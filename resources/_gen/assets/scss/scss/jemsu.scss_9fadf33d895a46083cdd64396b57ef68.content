/* Regular text */
@import url("https://fonts.googleapis.com/css2?family=Source+Serif+Pro:wght@400;700&display=swap");
body {
  font-family: "Source Serif Pro", Georgia, serif;
  font-weight: 400; }

h1 small,
h2 small,
h3 small,
h4 small {
  font-size: 60%; }

/* Links */
a {
  font-weight: 400; }

/* inline-code */
h1 code,
h2 code,
h3 code,
h4 code {
  font-size: inherit; }

/* Littlefoot */
div.littlefoot-footnote__wrapper > div > p {
  color: #212121; }

/* Mathjax fixes in dark/light mode because in Rmd it's enclosed in <code> */
/* Slightly compacter tables */
table th, table td {
  padding: 1.2rem; }

table caption {
  padding-bottom: 1rem; }

/* Sans-font for headings */
table th {
  font-family: Lato, Helvetica, sans-serif; }

/* Centered tables by default */
table {
  margin-left: auto;
  margin-right: auto; }

